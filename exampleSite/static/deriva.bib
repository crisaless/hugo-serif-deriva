	number = {17},
	urldate = {2019-08-29},
	journal = {Development},
	author = {Rutledge, Elisabeth A. and Benazet, Jean-Denis and McMahon, Andrew P.},
	month = sep,
	year = {2017},
	pmid = {28705898},
	pages = {3177--3188},
	file = {Full Text PDF:/Users/cristina/Zotero/storage/F7WY3AMU/Rutledge et al. - 2017 - Cellular heterogeneity in the ureteric progenitor .pdf:application/pdf;Snapshot:/Users/cristina/Zotero/storage/YEC457TN/3177.html:text/html},
