	keywords = {Animals, Mice, Oligonucleotide Array Sequence Analysis, Signal Transduction, Cells, Cultured, Humans, Mice, Knockout, Hair Follicle, Gene Expression Regulation, Microarray Analysis, Polymerase Chain Reaction, Cell Proliferation, Cell Cycle, Luminescent Measurements, STAT5 Transcription Factor},
	pages = {1781--1791},
}

