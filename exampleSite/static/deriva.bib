	author = {Xu, Kun and Wu, Xinyu and Shapiro, Ellen and Huang, Honging and Zhang, Lixia and Hickling, Duane and Deng, Yan and Lee, Peng and Li, Juan and Lepor, Herbert and Grishina, Irina},
