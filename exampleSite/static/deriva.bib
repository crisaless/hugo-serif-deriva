	keywords = {Computer Science - Human-Computer Interaction},
	file = {arXiv Fulltext PDF:/Users/cristina/Zotero/storage/9JJG6RAJ/Tangmunarunkit et al. - 2021 - Model-Adaptive Interface Generation for Data-Drive.pdf:application/pdf;arXiv.org Snapshot:/Users/cristina/Zotero/storage/BYGNFEDV/2110.html:text/html},
}

