	journal = {Mammalian Genome},
	author = {Smith, Constance M. and Finger, Jacqueline H. and Hayamizu, Terry F. and McCright, Ingeborg J. and Xu, Jingxia and Eppig, Janan T. and Kadin, James A. and Richardson, Joel E. and Ringwald, Martin},
