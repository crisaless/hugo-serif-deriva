	month = apr,
	year = {2012},
	pmid = {22290330},
	pmcid = {PMC3310225},
	keywords = {Animals, Gene Expression Regulation, Developmental, Rats, Kidney Tubules, Cyclic AMP-Dependent Protein Kinases, Epithelial Cells, Wnt Signaling Pathway, Extracellular Matrix, Coculture Techniques},
	pages = {11--21},
