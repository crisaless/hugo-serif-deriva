	pmid = {21336810},
	keywords = {Animals, Mice, Gene Expression Profiling, Gene Expression Regulation, Developmental, Kidney, Organogenesis, Humans, Mice, Transgenic, Databases, Genetic},
	pages = {1469--1478},
}

