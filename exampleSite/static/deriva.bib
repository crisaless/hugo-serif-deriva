@inproceedings{hosseini_hipaa_2016,
	title = {{HIPAA} compliant wireless sensing smartwatch application for the self-management of pediatric asthma},
	doi = {10.1109/BSN.2016.7516231},
	abstract = {Asthma is the most prevalent chronic disease among pediatrics, as it is the leading cause of student absenteeism and hospitalization for those under the age of 15. To address the significant need to manage this disease in children, the authors present a mobile health (mHealth) system that determines the risk of an asthma attack through physiological and environmental wireless sensors and representational state transfer application program interfaces (RESTful APIs). The data is sent from wireless sensors to a smartwatch application (app) via a Health Insurance Portability and Accountability Act (HIPAA) compliant cryptography framework, which then sends data to a cloud for real-time analytics. The asthma risk is then sent to the smartwatch and provided to the user via simple graphics for easy interpretation by children. After testing the safety and feasibility of the system in an adult with moderate asthma prior to testing in children, it was found that the analytics model is able to determine the overall asthma risk (high, medium, or low risk) with an accuracy of 80.10±14.13\%. Furthermore, the features most important for assessing the risk of an asthma attack were multifaceted, highlighting the importance of continuously monitoring different wireless sensors and RESTful APIs. Future testing this asthma attack risk prediction system in pediatric asthma individuals may lead to an effective self-management asthma program.},
	booktitle = {2016 {IEEE} 13th {International} {Conference} on {Wearable} and {Implantable} {Body} {Sensor} {Networks} ({BSN})},
	author = {Hosseini, A. and Buonocore, C. M. and Hashemzadeh, S. and Hojaiji, H. and Kalantarian, H. and Sideris, C. and Bui, A. A. T. and King, C. E. and Sarrafzadeh, M.},
	month = jun,
	year = {2016},
	keywords = {asthma attack risk prediction system, biomedical communication, chronic disease, cloud computing, cryptography, cryptography framework, diseases, Encryption, environmental wireless sensors, Health Insurance Portability and Accountability Act, HIPAA, medical computing, mHealth system, mobile computing, mobile health system, mobile radio, patient monitoring, pediatric asthma individuals, Pediatrics, physiological wireless sensors, Real-time systems, representational state transfer application program interfaces, RESTful API, self-management asthma program, Sensors, student absenteeism, watches, Wireless communication, wireless sensing smartwatch application, wireless sensor networks},
	pages = {49--54},
