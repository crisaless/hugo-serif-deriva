	pmcid = {PMC2756349},
	keywords = {Animals, Mice, Gene Expression Regulation, Developmental, Kidney, Mice, Inbred C57BL, Genetic Variation, Aquaporins, Mice, Inbred Strains, Multigene Family},
	pages = {745--759},
	file = {Full Text:/Users/cristina/Zotero/storage/HKERNAM9/Parreira et al. - 2009 - Expression patterns of the aquaporin gene family d.pdf:application/pdf},
}

