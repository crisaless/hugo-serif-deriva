	keywords = {Branching morphogenesis, Glomerulogenesis, Kidney development, Nephron induction},
	pages = {1--41},
	file = {Springer Full Text PDF:/Users/cristina/Zotero/storage/H696UAK8/Bates et al. - 2014 - Embryonic Development of the Kidney.pdf:application/pdf},
}

@article{hishikawa_decreased_2019,
