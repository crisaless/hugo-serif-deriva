	keywords = {Female, Humans, Male, Abdominal wall musculature, Adult, Copy number variant, DNA Copy Number Variations, Eagle-barrett syndrome, Genotype, Infant, Newborn, Phenotype, Prune belly syndrome, Prune Belly Syndrome, Sequence Analysis, DNA, Young Adult},
