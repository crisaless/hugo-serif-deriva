	number = {4},
	urldate = {2024-08-14},
	journal = {The Journal of Pathology},
	author = {Joseph, Diya B and Henry, Gervaise H and Malewska, Alicia and Reese, Jeffrey C and Mauck, Ryan J and Gahan, Jeffrey C and Hutchinson, Ryan C and Mohler, James L and Roehrborn, Claus G and Strand, Douglas W},
	month = apr,
	year = {2022},
	pages = {427--441},
	file = {Accepted Version:/Users/cristina/Zotero/storage/Z96GASAF/Joseph et al. - 2022 - 5‐Alpha reductase inhibitors induce a prostate lum.pdf:application/pdf},
}

