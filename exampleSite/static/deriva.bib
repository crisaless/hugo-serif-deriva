	pages = {1318--1332.e4},
	file = {ScienceDirect Full Text PDF:/Users/cristina/Zotero/storage/2UXA8P6V/Hishikawa et al. - 2019 - Decreased KAT5 Expression Impairs DNA Repair and I.pdf:application/pdf;ScienceDirect Snapshot:/Users/cristina/Zotero/storage/ETSV3DAC/S2211124719300051.html:text/html},
