	pmcid = {PMC4197071},
	keywords = {Animals, Kidney, Humans, Reverse Transcriptase Polymerase Chain Reaction, RNA, Messenger, Genetic Variation, Transcription, Genetic, Mice, Inbred DBA, Sequence Analysis, RNA, Alternative Splicing, Exons, Mutagenesis, Site-Directed, Receptors, Cell Surface},
