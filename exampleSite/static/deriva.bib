	keywords = {Animals, Mice, Gene Expression Profiling, Gene Expression Regulation, Developmental, Kidney, Signal Transduction, Stem Cells, Humans, Fetus, Cell Lineage, Branching, Fetal, Human, Single cell},
	file = {Full Text:/Users/cristina/Zotero/storage/JILYLJZZ/Menon et al. - 2018 - Single-cell analysis of progenitor cell dynamics a.pdf:application/pdf},
}

@article{patel_genetics_2013,
	title = {The genetics and epigenetics of kidney development},
