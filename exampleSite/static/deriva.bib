	month = dec,
	year = {2018},
	keywords = {benign prostatic hyperplasia, flow cytometry, GUDMAP, human cell atlas, human prostate, prostate cancer, prostate epithelia, prostate stroma, single-cell RNA sequencing, zonal anatomy},
	pages = {3530--3542.e5},
	file = {ScienceDirect Full Text PDF:/Users/cristina/Zotero/storage/PM93A6SP/Henry et al. - 2018 - A Cellular Anatomy of the Normal Adult Human Prost.pdf:application/pdf;ScienceDirect Snapshot:/Users/cristina/Zotero/storage/X3ZMEMYZ/S2211124718318771.html:text/html},
