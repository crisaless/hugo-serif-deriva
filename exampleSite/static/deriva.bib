	author = {Brophy, Patrick D. and Rasmussen, Maria and Parida, Mrutyunjaya and Bonde, Greg and Darbro, Benjamin W. and Hong, Xiaojing and Clarke, Jason C. and Peterson, Kevin A. and Denegre, James and Schneider, Michael and Sussman, Caroline R. and Sunde, Lone and Lildballe, Dorte L. and Hertz, Jens Michael and Cornell, Robert A. and Murray, Stephen A. and Manak, J. Robert},
