	journal = {The FASEB Journal},
	author = {Fontana, Jacopo M. and Khodus, Georgiy R. and Unnersjö-Jess, David and Blom, Hans and Aperia, Anita and Brismar, Hjalmar},
	month = nov,
	year = {2018},
	pages = {4089--4096},
	file = {Full Text PDF:/Users/cristina/Zotero/storage/QAJKXDW2/Fontana et al. - 2018 - Spontaneous calcium activity in metanephric mesenc.pdf:application/pdf;Snapshot:/Users/cristina/Zotero/storage/JXEV4N4D/fj.html:text/html},
}

