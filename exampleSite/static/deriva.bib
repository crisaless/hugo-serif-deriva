	author = {Menon, Rajasree and Otto, Edgar A. and Kokoruda, Austin and Zhou, Jian and Zhang, Zidong and Yoon, Euisik and Chen, Yu-Chih and Troyanskaya, Olga and Spence, Jason R. and Kretzler, Matthias and Cebrián, Cristina},
